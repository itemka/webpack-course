{
  "name": "webpack-course",
  "version": "1.0.0",
  "description": "webpack course",
  "privet": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --mode development --config webpack.config.js",
    "build": "cross-env NODE_ENV=prodaction webpack --mode production --config webpack.config.js",
    "watch": "cross-env NODE_ENV=development webpack --mode development --watch --config webpack.config.js",
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --config webpack.config.js --open",
    "stats": "webpack --json > stats.json && webpack-bundle-analyzer stats.json"
  },
  "browserslist": "> 0.25%, not dead",
